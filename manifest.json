{
  "name": "Trackmojis",
  "description": "ðŸ‘‹ ðŸ’¬ ðŸ‘‰ ðŸ’¾ Inserts emojis into Matomo to give a better visual understanding of tracked events",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "*://*/*?module=CoreHome*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*?module=CoreHome*"
      ],
      "run_at": "document_end",
      "all_frames": true,
      "js": [
        "addMatomojis.js"
      ]
    }
  ],
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  }
}